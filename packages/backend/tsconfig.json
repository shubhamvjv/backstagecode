{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
